le='{\"border\":\"1px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"1px 2px 3px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"radial-gradient(circle, rgba(246,246,246,1) 0%, rgba(230,230,230,1) 100%)\"}' label=\"管理账号\" prop=\"guanlizhanghao\">\r\n            <el-input v-model=\"ruleForm.guanlizhanghao\" \r\n                placeholder=\"管理账号\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"1px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"1px 2px 3px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"radial-gradient(circle, rgba(246,246,246,1) 0%, rgba(230,230,230,1) 100%)\"}' label=\"管理姓名\" prop=\"guanlixingming\">\r\n            <el-input v-model=\"ruleForm.guanlixingming\" \r\n                placeholder=\"管理姓名\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"1px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"1px 2px 3px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"radial-gradient(circle, rgba(246,246,246,1) 0%, rgba(230,230,230,1) 100%)\"}' label=\"备注\" prop=\"beizhu\">\r\n            <editor \r\n                :style='{\"minHeight\":\"300px\",\"padding\":\"0\",\"margin\":\"0\",\"borderColor\":\"#ccc\",\"backgroundColor\":\"#f9f9f9\",\"borderRadius\":\"0\",\"borderWidth\":\"0\",\"width\":\"100%\",\"borderStyle\":\"solid\",\"height\":\"auto\"}'\r\n                v-model=\"ruleForm.beizhu\" \r\n                class=\"editor\" \r\n                action=\"file/upload\">\r\n            </editor>\r\n          </el-form-item>\r\n\r\n      <el-form-item :style='{\"padding\":\"0\",\"margin\":\"20px 0 20px 0\"}'>\r\n        <el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0\",\"boxShadow\":\"1px 2px 6px #52a1db\",\"margin\":\"0 20px 0 0\",\"color\":\"rgba(255, 255, 255, 1)\",\"outline\":\"none\",\"borderRadius\":\"30%\",\"background\":\"ra